<mapper namespace="com.team2.mvc.mapper.MemberMapper">
	  <insert id="register" parameterType="com.team2.mvc.entity.Member">
     insert into
    	user(UNO,ID,PASSWORD,NAME,NICKNAME,EMAIL,ROLE)
     values(#{UNO},#{ID},#{PASSWORD},#{NAME},#{NICKNAME},#{EMAIL},#{ROLE})
  </insert>
	
</mapper>